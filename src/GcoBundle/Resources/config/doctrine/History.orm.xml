<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GcoBundle\Entity\History" table="history">
    <id name="userId" type="integer" column="user_id"/>
    <id name="techId" type="integer" column="tech_id"/>
    <field name="levelId" type="integer" column="level_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="statusId" type="integer" column="status_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
